build: sudo docker build -t kindssd .

to run: 

sudo docker run -it --rm \
  -p 8888:8888 \
  -v $(pwd)/your-code:/app/your-code \  # Mount your local code directory
  kindssd
  
  
  sudo docker run -it --rm \
  -v /home/Aamir/kindssd/github/KindModel:/app/KindModel \
  kindssd
  
  root@76c4548a92e5:/app# python -m KindModel.overcooked_marl.main
  
  
